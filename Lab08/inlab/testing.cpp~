using namespace std;
void methodbyvalue(int a, char b, int* c, float d);
void methodbyreference(int &a, char &b, int* &c, float &d);
void arraybyvalue(int arr[3]);
void arraybyreference(int (&arr)[3]);


main(){
 
  int a = 6;
  char b = 'b';
  int* c = &a;
  float d = 1.0;
  
  int arr [3] = {0, 1, 2};

  methodbyvalue(a, b, c, d);
  
  methodbyreference(a, b, c, d);



  return 0;


}



